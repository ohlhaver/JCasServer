#!/usr/bin/env ruby

require File.expand_path(File.dirname(__FILE__)+"/../lib/casserver/load_picnic")

require 'picnic/cli'

cli = Picnic::Cli.new(
  'rubycas-server',
  :app_module => 'CASServer',
  :app_file => File.expand_path(File.dirname(__FILE__)+"/../lib/casserver.rb")
)

cli.handle_cli_input
